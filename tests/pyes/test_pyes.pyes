def test_while_break_nobreak():
    x = 0
    mientras x < 2:
        x += 1
    nointerrumpir:
        x = -1

    afirmar x == -1

    mientras Verdadero:
        x += 1
        si x == 2:
            interrumpir
    nointerrumpir:
        x = -1

    afirmar x == 2


def test_import():
