def test_for_in_range_continue():
    result = []
    para i en intervalo(4):
        si i == 2:
            continuar
        result.append(i)
    afirmar result == [0, 1, 3]


def test_try_except_else_finally():
    intentar:
        a = b
    excepto NameError:
        b = 1
    finalmente:
        c = 2

    afirmar b == 1
    afirmar c == 2

    intentar:
        a = b
    excepto NameError:
        a = 3
    sino:
        a = c

    afirmar a == 2


def test_del():
