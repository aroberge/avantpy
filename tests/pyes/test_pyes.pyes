def test_repeat():
    x = 0
    repetir 4:
        x += 1
    afirmar x == 4

    repetir mientras x != 6:
        x += 1
    afirmar x == 6

    repetir hasta x == 8:
        x += 1
    afirmar x == 8

    repetir parasiempre:
        x += 1
        si x == 10:
            interrumpir
    afirmar x == 10


