    afirmar b == 2


def test_global_nonlocal():
    global CONSTANT
    CONSTANT += 1
    afirmar CONSTANT == 0
    x = 3

    def inner():
        nolocal x
        x += 1

    inner()
    afirmar x == 4

    def other():
        x = 5

    other()
    afirmar x == 4


